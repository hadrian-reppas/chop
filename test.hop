import pair
import pair::{square_pair ..fst .snd}

fn main {
    (double(one) + square_add(1.5, five_halves)) putln
    (one + 2*five_halves) putln
    "Hello, World!" putlns
    3 square_pair print
}

fn one -> int {
    1
}

fn five_halves -> float {
    2.5
}

fn double int -> int {
    2 *
}

fn square_add float float -> float {
    . * +
}

fn add int int -> int {
    +
}

fn print pair::Pair[int] {
    "Pair(" puts
    ..fst put
    ", " puts
    .snd put
    ')' putlnc
}
