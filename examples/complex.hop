struct Complex {
    real: float
    imag: float
}

fn + Complex Complex -> Complex {
    let a b {
        a .real b .real +
        a .imag b .imag +
        Complex
    }
}

fn put Complex {
    let z {
        z .real put
        " + " puts
        z .imag put
        'j' putc
    }
}

